<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:gravity="center_horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".HomeScreen" >

    <TextView
        android:id="@+id/versionNumberMainMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="34dp"
        android:text="Version Number"
        android:textSize="10sp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/menuOpenButton"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:text="Menu"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="@color/border_grey"
        android:textSize="100sp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:minHeight="375dp"
        android:minWidth="375dp"
        android:src="@drawable/cirdles_logo" />

    <Button
        android:id="@+id/menuOpenButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/menuHistoryButton"
        android:layout_alignLeft="@+id/menuHistoryButton"
        android:layout_marginBottom="27dp"
        android:background="@color/button_blue"
        android:minHeight="75dp"
        android:minWidth="300dp"
        android:text="Open"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="50sp" />

    <Button
        android:id="@+id/menuProfileButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/versionNumberMainMenu"
        android:layout_alignLeft="@+id/menuHistoryButton"
        android:layout_marginBottom="27dp"
        android:background="@color/button_blue"
        android:minHeight="75dip"
        android:minWidth="300dip"
        android:text="Profile"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="50sp" />

    <Button
        android:id="@+id/menuHistoryButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/menuProfileButton"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="23dp"
        android:background="@color/button_blue"
        android:minHeight="75dp"
        android:minWidth="300dp"
        android:text="History"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        android:textSize="50sp" />
    
</RelativeLayout>
