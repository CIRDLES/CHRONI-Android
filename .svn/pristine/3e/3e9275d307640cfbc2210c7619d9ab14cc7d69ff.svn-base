<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cirdles"
    android:versionCode="1"
    android:versionName="786" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />
    
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.WAKE_LOCK" />
    	<uses-permission android:name="android.permission.INTERNET" />
    	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cirdles_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" 
        android:debuggable="false">
        
        <activity
            android:name="com.example.cirdles.HomeScreen"
             android:screenOrientation="portrait"
             android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.cirdles.MainMenu"
            android:parentActivityName="android.intent.action.USERPROFILE"
           android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAINMENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
                <activity
            android:name="com.example.cirdles.UserProfile"
            android:parentActivityName="android.intent.action.MAINMENU"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.USERPROFILE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
         <activity
            android:name="com.example.cirdles.History"
            android:parentActivityName="android.intent.action.MAINMENU"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.HISTORY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity 
           android:name=".FilePickerActivity"
           android:screenOrientation="portrait"
           android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.FILEPICKER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.cirdles.AliquotMenu"
            android:parentActivityName="android.intent.action.MAINMENU"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">
            <intent-filter>
                <action android:name="android.intent.action.ALIQUOTMENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.cirdles.ReportSettingsMenu"
            android:parentActivityName="android.intent.action.MAINMENU"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.REPORTSETTINGSMENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

         <activity
            android:name="com.example.cirdles.Display"
            android:parentActivityName="android.intent.action.DISPLAY"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.DISPLAY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>

</manifest>
